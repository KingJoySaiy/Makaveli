<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Table.TCourse" table="course" schema="selectCourse">
        <id name="cId" column="CID"/>
        <property name="cName" column="CNAME"/>
        <property name="teacher" column="TEACHER"/>
    </class>

    <class name="Table.TCourseDemo" table="courseDemo" schema="selectCourse">
        <id name="cId" column="CID"/>
        <property name="sWeek" column="SWEEK"/>
        <property name="eWeek" column="EWEEK"/>
        <property name="day" column="DAY"/>
        <property name="location" column="LOCATION"/>
        <property name="sTime" column="STIME"/>
        <property name="length" column="LENGTH"/>
    </class>

    <class name="Table.TStudent" table="student" schema="selectCourse">
        <id name="sId" column="SID"/>
        <property name="password" column="PASSWORD"/>
        <property name="name" column="NAME"/>
        <property name="sClass" column="SCLASS"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Table.TCourseDemo" table="courseDemo" schema="selectCourse">
        <id name="cId" column="CID"/>
        <property name="sWeek" column="SWEEK"/>
        <property name="eWeek" column="EWEEK"/>
        <property name="day" column="DAY"/>
        <property name="location" column="LOCATION"/>
        <property name="sTime" column="STIME"/>
        <property name="length" column="LENGTH"/>
    </class>
</hibernate-mapping>